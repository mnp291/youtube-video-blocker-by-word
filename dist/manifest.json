{
	"manifest_version": 2,
	"name": "Youtube Blocker",
	"description": "Блокировать карточки с видио на youtube по ключевыи словам",
	"version": "1.0.0",
	"browser_action": {
		"default_icon": "icon.svg",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["*://www.youtube.com/*"],
          "js": ["./bundle.js"]
        }
      ],	
      "permissions": ["*://www.youtube.com/*", "storage"]
}